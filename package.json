{
  "name": "bettersearch",
  "displayName": "BetterSearch",
  "description": "General-purpose motion plugin inspired by leap.nvim",
  "icon": "media/icon.png",
  "version": "0.0.1",
  "publisher": "MurlocCra4ler",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "leap",
    "leap.nvim",
    "jump",
    "jumpy",
    "search",
    "better"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MurlocCra4ler/vscode-leap.git"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "leap.search",
        "title": "Leap Search: Open Widget"
      }
    ],
    "keybindings": [
      {
        "command": "leap.search",
        "key": "ctrl+cmd+option+shift+f"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.88.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  }
}